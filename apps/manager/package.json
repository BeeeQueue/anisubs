{
  "name": "@anisubs/manager",
  "private": true,
  "version": "0.0.0",
  "description": "TODO: description",
  "author": "BeeeQueue",
  "homepage": "",
  "engines": {
    "node": ">=12"
  },
  "lint-staged": {
    "*.{ts,js,vue,html,json,yml}": "prettier --write",
    "*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --no-deps -r dotenv/config -r tsconfig-paths/register --files --inspect=3009 -- src/index.ts",
    "migrate": "yarn typeorm migration:run",
    "migrate:create": "yarn typeorm migration:create --name",
    "precommit": "lint-staged",
    "start": "yarn ts-node -T -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "typeorm": "ts-node -T -r dotenv/config -r tsconfig-paths/register ../../node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@anisubs/shared": "*",
    "@koa/cors": "3.1.0",
    "anitomy-js": "5.0.0",
    "apollo-server-koa": "2.21.1",
    "bottleneck": "2.19.5",
    "bullmq": "1.15.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dataloader": "2.0.0",
    "envsafe": "2.0.3",
    "fuse.js": "6.4.6",
    "got": "11.8.2",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-helmet": "6.1.0",
    "koa-pino-logger": "3.0.0",
    "koa-router": "10.0.0",
    "koa-static-server": "1.5.2",
    "node-cache": "5.1.2",
    "nyaapi": "2.4.1",
    "pino": "6.11.2",
    "pino-pretty": "4.3.0",
    "pg": "8.5.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "type-graphql": "1.1.1",
    "typedi": "0.8.0",
    "typeorm": "0.2.30",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/koa": "2.11.6",
    "@types/koa-helmet": "6.0.2",
    "@types/koa-pino-logger": "3.0.0",
