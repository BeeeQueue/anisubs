    "got": "11.8.1",
    "koa": "2.13.1",
    "koa-router": "10.0.0",
    "rimraf": "3.0.2",
    "subtitle": "4.0.1"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "2.1.16",
